package backjoon;
import java.util.*;
public class Q_2448_별11 {
	
	public ArrayList <String> base_star() {
		ArrayList <String> bs = new ArrayList<String>();
		for(int i=0; i<6;i++) {
			if (i%2 == 0) { //아무것도 출력 안됨,
				bs.add(i,"     ");
			}else {
				if(i == 1) {
					bs.add(i,"  *  ");
				}else if(i == 3) {
					bs.add(i," * * ");
				}else if(i ==5 ) {
					bs.add(i,"*****");
				}
			}
		}
		return bs;
		
	}
	
	//가장 작은 단위의 삼각형 그리기 
	public static void main(String[] args) {
		int count_empty = 0;
		
		System.out.println("입력된 N의 값 : " + 3);
		System.out.println("count_empty :" + count_empty);
		
	}

}
